{
  "name": "resume",
  "version": "0.0.1",
  "description": "",
  "main": "gulpfile.js",
  "scripts": {
    "clean:css": "rimraf assets/css/*.css",
    "build:css": "npm run clean:css && postcss -c postcss.config.js",
    "reload:css": "browser-sync reload --port 4000 --files='assets/css/*.css'",
    "watch:css": "watch 'npm run build:css && npm run reload:css' src/css",
    "clean:js": "rimraf assets/js/*.js",
    "build:js": "npm run clean:js && webpack",
    "reload:js": "browser-sync reload --port 4000 --files='assets/js/*.js'",
    "watch:js": "watch 'npm run build:js && npm run reload:js' src/js",
    "build": "npm run build:css & npm run build:js",
    "server": "browser-sync start --config bs.config.js",
    "watch": "npm run watch:js & npm run watch:css & npm run server"
  },
  "author": "Knovour",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-runtime": "^6.5.0",
    "browser-sync": "^2.11.1",
    "cssnano": "^3.5.2",
    "postcss": "^5.0.17",
    "webpack": "^1.12.13"
  },
  "dependencies": {
    "elemental": "^0.5.13",
    "material-design-iconic-font": "^2.2.0",
    "material-ui": "^0.14.4",
    "react": "^0.14.7",
    "react-addons-css-transition-group": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.0",
    "react-tap-event-plugin": "^0.2.2",
    "redux": "^3.3.1"
  }
}
